{
  "name": "@sieutoc/themes",
  "version": "2.0.6",
  "description": "Theme collection for our template",
  "author": "Sieutoc Team",
  "license": "MIT",
  "homepage": "https://github.com/websitesieutoc/themes#README",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/websitesieutoc/themes.git"
  },
  "bugs": {
    "url": "https://github.com/websitesieutoc/themes/issues"
  },
  "scripts": {
    "changeset": "changeset",
    "build": "tsup src --dts",
    "dev": "pnpm build:fast -- --watch",
    "clean": "rimraf node_modules dist",
    "lint": "eslint ./src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "build:fast": "tsup src",
    "ci:version": "changeset version",
    "ci:publish": "changeset publish"
  },
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "format": [
      "cjs",
      "esm"
    ],
    "target": "es2019",
    "sourcemap": true
  },
  "dependencies": {
    "@chakra-ui/react": "2.8.0",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.2",
    "@typescript-eslint/eslint-plugin": "6",
    "@typescript-eslint/parser": "6",
    "eslint": "8.48.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "prettier": "3.0.3",
    "rimraf": "5.0.1",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  }
}
